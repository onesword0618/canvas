{
  "name": "canvas",
  "version": "0.0.1",
  "description": "Convert SVG File to ICO Format File.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "start": "tsc && node build/src/index.js",
    "typedoc": "typedoc --hideGenerator --out ./docs/api/ ./src/",
    "lint": "eslint --ext .js,.ts src/",
    "format": "prettier --write src/**/*",
    "test": "jest --ci --reporters=default --reporters=jest-junit --testResultsProcessor=jest-junit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onesword0618/canvas.git"
  },
  "keywords": [
    "image",
    "convert",
    "svg",
    "ico",
    "png"
  ],
  "author": "onesword0618 <ao.akua.leo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onesword0618/canvas/issues"
  },
  "homepage": "https://github.com/onesword0618/canvas#readme",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "@types/sharp": "^0.29.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "sharp": "^0.29.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
